<div class="container">
  <h1 style="margin-left:30px">News</h1>
  <div class="span8">
    <div class="blog roundme" style="background-color:#303030;color:#FFF0F5;">
      <div class="dates"><h4><%= @news.created_at.strftime("%B %d, %Y")%></h4></div>
        <h2><%= @news.title%></h2>
        <% if !@news.image.blank?%> <div class=""><%= image_tag @news.image %></div><% end%>
        <%= @news.body%>
      </div>
  </div>
  <div class="otherpages">
    <%= render :partial => "shared/sidebar"%>
  </div>
</div>